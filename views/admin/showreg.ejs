<%- include('../partials/header')%>
    <%- include('navbar')%>
        <!-- <div id="preloader"><div id="status"><div class="spinner"></div></div></div> -->
        
        <div class="container mt-3">
            <% if(data.length==0){ %>
                <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
                <script>
                    swal("No registration yet!", "No data to be shown", "warning");
                </script>
                <%}%>
                    <div class="container d-flex justify-content-end align-items-center">
                        <% var filename = eventname + Date.now().toString() + 'xls' %>
                        <button class="btn btn-danger" id="exporttable" onclick="exportTableToExcel('myTable', '<%=filename%>')"><i class="fas fa-download"></i>
                            Download</button>
                    </div>
                    <div class="container mt-3">
                        <table class="table table-striped" id="myTable">
                            <thead>
                                <tr>
                                    <th scope="col">#</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Email</th>
                                    <th scope="col">Phone</th>
                                    <th scope="col">Card Number</th>
                                    <th scope="col">Card Status</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% var count=1 %>
                                    <% data.forEach(element=>{ %>
                                        <tr>
                                            <th scope="row">
                                                <%=count %>
                                            </th>
                                            <td>
                                                <%=element.name%>
                                            </td>
                                            <td>
                                                <%=element.email%>
                                            </td>
                                            <td>
                                                <%=element.phone%>
                                            </td>
                                            <td>
                                                <%=element.cardno%>
                                            </td>
                                            <td>
                                                <%=element.status%>
                                            </td>
                                        </tr>
                                        <% count+=1 %>
                                            <% }) %>
                            </tbody>
                        </table>
                    </div>
        </div>
        
        <%- include('../partials/footer')%>